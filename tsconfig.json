{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@Layouts/*": ["src/shared/layouts/*"],
      "@Components/*": ["src/shared/components/*"],
      "@Templates/*": ["src/shared/templates/*"],
      "@Configs/*": ["src/configs/*"],
      "@Services/*": ["src/services/*"],
      "@Utils/*": ["src/utils/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
