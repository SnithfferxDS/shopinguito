---
import Mainlayout from "@Layouts/main.astro";
import Navbar from "@Templates/guest.navbar.astro";
import Footer from "@Templates/guest.footer.astro";

interface Props {
	title?: string;
}

const { title } = Astro.props;
---

<Mainlayout title={title}>
	<Navbar />
	<div class="mx-auto mt-6 px-2">
		<slot />
	</div>
	<Footer />
</Mainlayout>
